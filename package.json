{
  "name": "wb-tools-launcher",
  "version": "1.0.0",
  "description": "World Bank Tools Launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-win": "electron-builder --win",
    "build-portable": "electron-builder --win portable"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0"
  },
  "build": {
    "appId": "com.wbtools.launcher",
    "productName": "WB Tools",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "preload.js",
      "launcher.html",
      "servers/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "icon.ico",
      "asar": false
    }
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^2.0.2",
    "pdf-parse": "^1.1.1"
  }
}
